{
  "build": {
    "content": [
      {
        "files": [
          "api/**/*.md",
          "api/**/toc.yml",
          "guides/**/*.md",
          "guides/**/toc.yml",
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "templates/material"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "SkiaSharp.Extended",
      "_enableSearch": true,
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/logo.ico",
      "_gitContribute": {
        "repo": "https://github.com/mono/SkiaSharp.Extended"
      },
      "_gitUrlPattern": "github"
    }
  }
}